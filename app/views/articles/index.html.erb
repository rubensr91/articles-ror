<div class="container">
    <h1>My articles</h1>

    <% @articles.each do |article| %>
        <div class="container">
            <h3 class="text-uppercase"><%=article.id %>. <%=article.title %></h3>
            <div>
                <%= link_to "Show", "articles/#{article.id}" %>
            </div>
        </div>
    <%end%>
</div>